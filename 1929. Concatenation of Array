
class Solution {
    public int[] getConcatenation(int[] nums) {
        int n = nums.length ; 
        int[] arr = new int[2*n] ; 
        int n1 = 0 ;
        for(int i = 0 ; i < n ; i ++){
            arr[n1] = nums[i] ; 
            n1 ++ ; 
        }
        for(int i = 0 ; i < n ; i ++){
            arr[n1] = nums[i] ; 
            n1 ++ ; 
        }
        return arr ; 
    }
}
