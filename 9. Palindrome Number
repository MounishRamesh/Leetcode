
class Solution {
    public boolean isPalindrome(int x) {
        int n = x ; 
        int reverse = 0 ; 
        while(x > 0){
            int n1 = x % 10 ; 
            reverse = (reverse*10) + n1 ; 
            x = x / 10 ; 
        }
        if(n == reverse){
            return true ; 
        } 
        else
            return false ; 
        
    }
}
